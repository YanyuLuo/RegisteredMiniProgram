.sliderContent{
  position: relative;
  margin: 0 auto;
  margin-bottom: 50rpx;
  padding-left: 60rpx;
  width: 455rpx;
  box-sizing: border-box;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 60rpx;
  background-color: #fff;
  color: #289adc;
  box-shadow: 0px 4px 6px 0px rgba(37, 114, 219, 0.3);
}
 
.sliderContent input {
  line-height: 90rpx;
  height: 90rpx;
  box-sizing: border-box;
  padding-left: 40rpx;
  width: 270rpx;
}
 
.input-placeholder {
  margin-left: 5px;
  text-align: center;
  color: #289adc;
}
 
 
